	.tab{
    display: flex;
    margin-top:30rpx ;
    width:100%;
   }
   .tab-item{
    flex:1;
    font-size:12pt ;
    text-align: center;
    line-height: 80rpx;
    border-bottom: 9rpx solid #eee;
   }
   .content{
    flex: 1;
   }
   .content>swiper{
    height: 100%;
   }
   .tab-item.on{
    color:#0164a6;
    border-bottom-color:#0164a6 ;
   }

   button[plain]{
    border: 0;
    width: 80px;
  }

  image{
    margin: 0;
    width: 80rpx;
    height: 80rpx;
    border: none;
  }

  .movable-area{
    pointer-events:none;
    z-index: 100;
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    opacity: 0.6;
  }
  .movable-view{ 
    pointer-events:auto;
    height: 50px;
    width: 50px;
    border-radius:50px;
    position:fixed; 
    background:#0164a6;
    line-height:50px;
    font-size:25pt;
    color:#fff;
  }
  .plus-position {
    margin-left: 30rpx;
    margin-bottom: 15rpx;
  }

  .demo{
    margin: 15rpx;
    background-color:#ffffff;
    box-shadow:0px 2px 4px 2px #DDDDDD;
    border-width: 0rpx;
    border-radius:15rpx;
    overflow:hidden;
  }
  .demo1{
    display: flex;
    flex-direction: row;
    margin: 15rpx;
    padding:15rpx;
    background-color:#ffffff;
    box-shadow:0px 2px 4px 2px #DDDDDD;
    border-width: 0rpx;
    border-radius:15rpx;
    overflow:hidden;
  }
  